# GNU Make workspace makefile autogenerated by Premake

.NOTPARALLEL:

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  create_config = debug
endif
ifeq ($(config),release)
  create_config = release
endif

PROJECTS := create

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

create:
ifneq (,$(create_config))
	@echo "==== Building create ($(create_config)) ===="
	@${MAKE} --no-print-directory -C build -f Makefile config=$(create_config)
endif

clean:
	@${MAKE} --no-print-directory -C build -f Makefile clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   create"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"